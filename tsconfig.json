{
  "extends": "expo/tsconfig.base",   // âœ… Add comma here
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native", 
    "lib": ["esnext"],
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "esnext",
    "module": "esnext",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/index.jsx", "app/(tabs)/_layout.jsx", "app/(tabs)/explore.jsx"  ]
}
